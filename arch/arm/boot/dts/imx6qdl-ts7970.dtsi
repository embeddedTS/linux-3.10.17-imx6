				MX6QDL_PAD_DISP0_DAT2__GPIO4_IO23      0x80000000 // MIC_DET
				MX6QDL_PAD_DISP0_DAT4__GPIO4_IO25      0x80000000 // IMX6_BLUE_LED
				MX6QDL_PAD_DISP0_DAT5__GPIO4_IO26      0x80000000 // EN_MODBUS_24V
				MX6QDL_PAD_DISP0_DAT9__GPIO4_IO30      0x80000000 // LCD_D09
				MX6QDL_PAD_DISP0_DAT10__GPIO4_IO31     0x80000000 // LCD_D10
				MX6QDL_PAD_DISP0_DAT11__GPIO5_IO05     0x80000000 // LCD_D11
				MX6QDL_PAD_DISP0_DAT12__GPIO5_IO06     0x80000000 // LCD_D12
				MX6QDL_PAD_DISP0_DAT13__GPIO5_IO07     0x80000000 // LCD_D13
				MX6QDL_PAD_DISP0_DAT14__GPIO5_IO08     0x80000000 // LCD_D14
				MX6QDL_PAD_DISP0_DAT15__GPIO5_IO09     0x80000000 // LCD_D15
				MX6QDL_PAD_DISP0_DAT16__GPIO5_IO10     0x80000000 // LCD_D16
				MX6QDL_PAD_DISP0_DAT17__GPIO5_IO11     0x80000000 // LCD_D17
				MX6QDL_PAD_DISP0_DAT18__GPIO5_IO12     0x80000000 // LCD_D18
				MX6QDL_PAD_DISP0_DAT19__GPIO5_IO13     0x80000000 // LCD_D19
				MX6QDL_PAD_DISP0_DAT20__GPIO5_IO14     0x80000000 // LCD_D20
				MX6QDL_PAD_DISP0_DAT23__GPIO5_IO17     0x80000000 // SEL_DC_USB#
				MX6QDL_PAD_EIM_LBA__GPIO2_IO27         0x80000000 // BUS_ALE#
				MX6QDL_PAD_EIM_RW__GPIO2_IO26          0x80000000 // JP_SD_BOOT#
				MX6QDL_PAD_EIM_CS0__GPIO2_IO23         0x80000000 // BUS_CS#
