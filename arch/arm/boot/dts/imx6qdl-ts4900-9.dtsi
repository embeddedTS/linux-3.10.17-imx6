				MX6QDL_PAD_EIM_DA12__EIM_AD12          0xa0f9 // MUX_AD_12
				MX6QDL_PAD_EIM_DA13__EIM_AD13          0xa0f9 // MUX_AD_13
				MX6QDL_PAD_EIM_DA14__EIM_AD14          0xa0f9 // MUX_AD_14
				MX6QDL_PAD_EIM_DA15__EIM_AD15          0xa0f9 // MUX_AD_15
			>;
